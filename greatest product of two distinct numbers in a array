#include<stdio.h>
int main()
{
 int n,a1,a2;
 scanf("%d",&n);
 int a[n];
 for(int i=0;i<n;i++)
{ int num;
	scanf("%d",&num);
	a[i]=num;
}
	for(int i=0;i<n;i++)
	{   int b=1;
		for(int j=0;j<n;j++)
		{
			if (i==j)
			continue;
			else if (a[i]>=a[j])
			b++;
		}
	if(b==n)	
	
		a1=a[i];
		
	

	if(b==n-1)
	a2=a[i];
	
	}
	printf("%d",a1*a2);
}
