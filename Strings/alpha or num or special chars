#include <stdio.h>

int main() 
{
    char str[100], num,alp,sc;
    gets(str);
    for (int i=0;str[i]!='\0'; i++) 
    {
        if ((str[i]>=97&&str[i]<=122)||(str[i]>=65&&str[i]<=90))
            alp++;
            else if(str[i]>=48&&str[i]<=57)
            num++;
            else
            sc++;
            
    }
	printf("numbers=%d\nalphabets=%d\nspecial characters=%d",num,alp,sc);

	return 0;
}
