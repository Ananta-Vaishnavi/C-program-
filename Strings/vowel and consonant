#include <stdio.h>

int main() 
{
    char str[100], c=0,v=0;
    gets(str);
    for (int i=0;str[i]!='\0'; i++) 
    {
        if (str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u')
            v++;
            else 
            c++;
    }
	printf("Vowel=%d",v);
	printf("\nConsonant%d",c);
	return 0;
}
