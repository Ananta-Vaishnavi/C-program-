	int a[r][c],b[r][c],re[r][c];
	for(int i=0;i<r;i++) 
	{
	for(int j=0;j<c;j++)
	 {
	 	scanf("%d",&a[i][j]);
         }
	
    }
    for(int i=0;i<r;i++) 
	{
	for(int j=0;j<c;j++)
	 {
	 	scanf("%d",&b[i][j]);
         }
	
    }
    for(int i=0;i<r;i++) 
	{
	for(int j=0;j<c;j++)
	 {
	 	if(i==j)
	 re[i][j]=a[i][j]+b[i][j];
    }
	
    }
     for(int i=0;i<r;i++) 
	{
	for(int j=0;j<c;j++)
	 {
	 	printf("%d ",re[i][j]);
         }
         	printf("\n");
}
}
