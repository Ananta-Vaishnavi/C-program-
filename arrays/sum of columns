#include <stdio.h>
int main()
{
  int r,c;
  scanf("%d %d",&r,&c);
  int a[r][c],sum[c]={0};
    {
      
       for(int i=0;i<r;i++)
        {
            for(int j=0;j<c;j++)
            {
	         scanf("%d",&a[i][j]);
            }
        }   
       for(int j=0;j<c;j++)
        {
            for(int i=0;i<c;i++)
            {
	         sum[j]+=a[i][j];
            }
            printf("\n%d",sum[j]);
        }   
   }
}
