#include<stdio.h>
int main()
{
 int n;
 scanf("%d",&n);
 int a[n];
 for(int i=0;i<n;i++)
{ int num;
	scanf("%d",&num);
	a[i]=num;
}
	for(int i=0;i<n;i++)
	{   int b=1;
		for(int j=1;j<n;j++)
		{
			if (i==j)
			continue;
			else (a[i]>=a[j]);
			b++;
		}
	if(b==n)	
	{
		printf("%d",a[i]);
	
	}
	}
}
