#include<stdio.h>
int arr(int a[],int n)
{
   for(int i=0;i<n;i++)
    {   int c=1;
        if(a[i]<a[i+1])
        c++ ;
        
        else
        {
            printf("not ascending");
            break;
        }
        if(c==1)
        printf("ascending");
    }
}

int main()
{
   int i,n,a[n];
    scanf("%d",&n);
    
    for(i=0;i<n;++i)
    {
        scanf("%d",&a[i]);
    }
    
    arr(a,n);
    
    return 0;
}
