#include<stdio.h>
int main()
    {
        int n;
        scanf("%d",&n);
        int a[7];
        for(int i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        printf("Enter the num you want to instered ");
        int numi,ins;
        scanf("%d",& numi);
        printf("enter the place you want to instert that in ");
        scanf("%d",&ins);
        
           for(int j=n;j>=ins-1;j--)
          {
            a[j+1]=a[j];
            
          }
          a[ins-1]=numi;
        for(int i=0;i<n+1;i++)
        {
        printf("%d ",a[i]);
        }    
        }
