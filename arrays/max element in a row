#include<stdio.h>
int main()
{
    int m,n; 
	prinf("enter number of rows and coloums");                
   scanf("%d %d",&m,&n);  
    int arr[m][n];   
		prinf("enter the elements");      
   for(int i=0;i<m;i++)    
    {
        for(int j=0;j<n;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
   
    int row,c=1;
    	prinf("enter the row");      
    scanf("%d", &row);
    
    for(int i=0;i<n;i++)    
    {   c=1;
        for(int j=0;j<n;j++)
        {   if(i==j)
            continue;
            else if (arr[row][i]>arr[row][j])
            c++;
        }
        if(c==n)
        printf("%d",arr[row][i]);
    }
   
}
