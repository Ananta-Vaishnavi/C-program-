#include<stdio.h>
int main()
	{
		int n;
		scanf("%d",&n);
		int a[n];
		for(int i=0;i<n;i++)
		{
		scanf("%d",&a[i]);
	    }
	    printf("Enter the num you want to delete");
	    int numd;
	    scanf("%d",& numd);
	    for(int i=0;i<n;i++)
	    {    
	      
	    	if(a[i]==numd)
	    	{
	       for(int j=i;j<n;j++)
	      {
	    	a[j]=a[j+1];
	    	
		  }}
		  if(a[i]!=a[n])
		  printf("%d",a[i]);
			
		} 

	}
