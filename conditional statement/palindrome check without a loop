#include<stdio.h>
 int main() 
 { 
 int n,a,b,c,d,e,f,rev; 
 printf("Enter a five digit number"); 
 scanf("%d",&n); 
 a=n%10; 
 b=(n%100)/10; 
 c=(n%1000)/100; 
 d=(n%10000)/1000; 
 e=n/10000; 
 rev=a*10000+b*1000+c*100+d*10+e; 
 printf("%d",rev); 
 if (n==rev) printf("\n yes"); 
 else printf("\n no"); 
 }
